{
  "name": "n8n-nodes-wasm-executor",
  "version": "0.1.0",
  "description": "",
  "keywords": [
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "homepage": "",
  "author": {
    "name": "Andreas Fitzek",
    "email": "andreas.fitzek@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/<...>/n8n-nodes-<...>.git"
  },
  "engines": {
    "node": ">=18.10",
    "pnpm": ">=9.1"
  },
  "packageManager": "pnpm@9.1.4",
  "main": "index.js",
  "scripts": {
    "cargo-build": "tsc && cargo build --message-format=json-render-diagnostics > cargo.log",
    "cross-build": "tsc && cross build --message-format=json-render-diagnostics > cross.log",
    "postcargo-build": "neon dist -n neon-wasm-module < cargo.log",
    "postcross-build": "neon dist -n neon-wasm-module -m /target < cross.log",
    "prepack": "tsc && neon update",
    "build": "npm run cargo-build -- --release && gulp build:icons",
    "cross": "npm run cross-build -- --release && gulp build:icons",
    "postbuild": "cp demo.wasm dist/",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "pnpm build && pnpm lint -c .eslintrc.prepublish.js nodes credentials package.json"
  },
  "files": [
    "dist",
    "index.node"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [],
    "nodes": [
      "dist/nodes/WasmExecutor/WasmExecutor.node.js"
    ]
  },
  "devDependencies": {
    "@neon-rs/cli": "^0.1.82",
    "@types/node": "^22.10.6",
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^8.56.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "n8n-workflow": "*",
    "prettier": "^3.3.2",
    "typescript": "^5.5.3"
  },
  "neon": {
    "type": "library",
    "org": "@neon-wasm-module",
    "platforms": "common",
    "load": "./src/load.cts"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "@neon-rs/load": "^0.1.82"
  },
  "optionalDependencies": {
    "@neon-wasm-module/win32-x64-msvc": "0.1.0",
    "@neon-wasm-module/darwin-x64": "0.1.0",
    "@neon-wasm-module/darwin-arm64": "0.1.0",
    "@neon-wasm-module/linux-x64-gnu": "0.1.0",
    "@neon-wasm-module/linux-arm64-gnu": "0.1.0"
  }
}
